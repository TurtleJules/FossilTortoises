file = {:C$\backslash$:/Users/Jule/Documents/Uni/MA/References{\_}MfN/MA{\_}Analysis/Maddison et al{\_}1997{\_}Nexus an extensible file format for systematic information.pdf:pdf},
isbn = {1063-5157},
issn = {1063-5157},
journal = {Systematic Biology},
number = {4},
pages = {590--621},
pmid = {11975335},
title = {{Nexus: An Extensible File Format for Systematic Information}},
url = {https://academic.oup.com/sysbio/article-lookup/doi/10.1093/sysbio/46.4.590},
volume = {46},
year = {1997}
}
