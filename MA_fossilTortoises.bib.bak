@article{Bover2008,
abstract = {The Balearic Islands are an archipelago located in the Western Mediterranean Sea. Their isolation from the mainland allowed the establishment of different faunas on each island. In Mallorca, the Pliocene fauna was composed of the so-called Myotragus-fauna (mainly consisting of a bovid, a glirid and a soricid). In Menorca, it was constituted by the giant rabbit-fauna (mainly consisting of a giant rabbit and a tortoise), and in the Pityusics by a tortoise, a lizard and two rodents. A main faunal turnover took place during the Late Pliocene or Early Pleistocene: the Myotragus-fauna reached Menorca and replaced the giant rabbit fauna. In the Pityusics, all mammals and the tortoise became extinct before the Late Pleistocene for unknown reasons, leaving birds and the lizard as the only vertebrates of these islands. Almost all the endemic vertebrates of the Balearics became extinct probably due to the first human arrival to the islands. ?? 2007 Elsevier Ltd and INQUA.},
author = {Bover, Pere and Quintana, Josep and Alcover, Josep Antoni},
doi = {10.1016/j.quaint.2007.06.039},
file = {:C$\backslash$:/Users/Jule/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Bover, Quintana, Alcover - 2008 - Three islands, three worlds Paleogeography and evolution of the vertebrate fauna from the Balearic Isl.pdf:pdf},
isbn = {1040-6182},
issn = {10406182},
journal = {Quaternary International},
number = {1},
pages = {135--144},
title = {{Three islands, three worlds: Paleogeography and evolution of the vertebrate fauna from the Balearic Islands}},
volume = {182},
year = {2008}
}
