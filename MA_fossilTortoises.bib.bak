keywords = {Chelonii,Cheloniidae,Dermochelyidae,Dermochelys coriacea sister taxon,Early messinian,Euclastes melii,Late buidigalian,Miocene,Pstphopborus polygonus,Reptilia,Southern Italy,Testudo varicosa,Trachyaspis lardyi},
number = {2},
pages = {321--333},
title = {{Fossil sea turtles (Chelonii, Dermochelyidae and Cheloniidae) from the Miocene of Pietra Leccese (late Burdigalian-early Messinian), Southern Italy}},
