@article{Mudar2007,
abstract = {This study reports on analysis of a sample of animal bones from Pleistocene levels of Lang Rongrien Rockshelter, Thailand. Analysis identified small proportions of marine and/or freshwater fish bone, freshwater/terrestrial snail shells, and bird bones, as well as large proportions of tortoise, turtle, and mammal bones. Comparison with three other faunal assemblages underscores salient characteristics consisting of a high proportion of turtle and tortoise and an absence of pigs in the Lang Rongrien sample. Analysis of the faunal assemblage suggests that, in contrast to other sites such as Niah Cave and Moh Khiew that were occupied on a long-term basis, the Pleistocene levels of Lang Rongrien were intermittently occupied by foragers who may have been practicing a seasonal round that involved transhumance from interior to coast. Keywords: Pleistocene, faunal analysis, climate reconstruction, pigs, subsistence, Thailand, Southeast Asia.},
author = {Mudar, Karen. and Anderson, Douglas D.},
doi = {10.1353/asi.2007.0013},
file = {:C$\backslash$:/Users/Jule/Documents/Uni/MA/References{\_}MfN/MA{\_}Turtles-tortoises as food/Mudar{\_}new evidence for southeast asian pleistocene foraging economies.pdf:pdf},
issn = {1535-8283},
journal = {Asian Perspectives},
number = {2},
pages = {298--334},
title = {{New Evidence for Southeast Asian Pleistocene Foraging Economies: Faunal Remains from the Early Levels of Lang Rongrien Rockshelter, Krabi, Thailand}},
url = {http://muse.jhu.edu/content/crossref/journals/asian{\_}perspectives/v046/46.2mudar.pdf},
volume = {46},
year = {2007}
}
@article{Rocek2013,
abstract = {Anurans of Laurasia have a long history that begins$\backslash$nwith the earliest known anuran, Prosalirus, from the Early$\backslash$nJurassic of Arizona, USA. At that time, western Laurasia$\backslash$n(North America) was still connected with Gondwana, so$\backslash$nfaunal interchange was still possible between those parts of$\backslash$nthe former Pangean supercontinent. The anuran fossil record$\backslash$nfrom the Jurassic and Cretaceous of Laurasia is mainly represented$\backslash$nby disarticulated skeletal elements similar to those of$\backslash$nProsalirus (e.g. amphicoelous vertebrae indicating the presence$\backslash$nof continuous notochord; ilia without dorsal crest and$\backslash$ndorsal tubercle; small body size). Because the morphology of$\backslash$nthe ilium, the most commonly preserved element ofMesozoic$\backslash$nanurans, superficially recalls that of Recent Alytes, Bombina$\backslash$nor Pelobates , Mesozoic anurans often were assigned to$\backslash$ndiscoglossids and pelobatids. The Cretaceous portion of the$\backslash$nLaurasian anuran record is marked by the appearance of$\backslash$nprocoelous and opisthocoelous vertebrae, ilia bearing a dorsal$\backslash$ncrest and dorsal tubercle (although such ilia may rarely be$\backslash$nfound as early as in the Jurassic) and larger body sizes.$\backslash$nCretaceous anuran assemblages include a mix of generalised$\backslash$ntaxa that are comparable to Recent basal anurans and more$\backslash$nspecialised taxa lacking clear affinities with any extant anurans.$\backslash$nSome of these forms survived into the Paleocene, but in$\backslash$ngeneral anuran faunas on all Laurasian continents were markedly$\backslash$ndepleted in the Paleocene. Major groups of anurans$\backslash$nappeared in the Eocene. The early Miocene is the interval$\backslash$nwhen Eurasian and American herpetofaunas reached their peak$\backslash$ntaxonomic diversities. In the Pliocene, some extant anuran species$\backslash$nappeared, but at the same time taxa that had been dominant$\backslash$nthroughout the Oligocene and Miocene (e.g. Eopelobates ,$\backslash$npalaeobatrachids) became extinct during this interval or during$\backslash$nthe subsequent Pleistocene glaciation. The brief biochronological$\backslash$nsynopsis presented here is followed by a systematic review of$\backslash$ntaxa with their diagnoses and published data on their stratigraphic$\backslash$nand geographic distributions.},
author = {Ro{\v{c}}ek, Zbyn{\v{e}}k},
doi = {10.1007/s12549-013-0131-y},
file = {:C$\backslash$:/Users/Jule/Documents/Uni/MA/References{\_}MfN/MA{\_}not useful/Rocek-2013-Mesozoic and Tertiary Anura of Laurasia.pdf:pdf},
issn = {18671594},
journal = {Palaeobiodiversity and Palaeoenvironments},
keywords = {Anura,Distribution,Laurasia,Stratigraphy,Systematic review,Taxonomic diagnoses},
number = {4},
pages = {397--439},
title = {{Mesozoic and tertiary anura of laurasia}},
volume = {93},
