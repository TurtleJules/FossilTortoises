@article{Leakey2011,
abstract = {Faunal evolution over the last 65 million years of earth's history was dominated by mammalian radiations, but much of this era is poorly represented in Africa. Mammals first appeared early in the Mesozoic, living alongside dinosaurs for millions of years, but it was not until the extinction of dinosaurs 65 myr ago that the first major explosion of mammalian taxa took place. The Cenozoic (65 Ma to Recent) witnessed repeated and dynamic events involving the radiation, evolution, and extinction of mammalian faunas. Two of these events, each marking the extinction of one diverse fauna and subsequent establishment of another equally diverse fauna, both involving advanced catarrhine primates, are recorded in sites in the Turkana Basin, despite the poorly represented record of Cenozoic faunas elsewhere in sub-Saharan Africa. The first of these events occurred at the Oligocene-Miocene transition and the other at the Miocene-Pliocene transition.},
author = {Leakey, Meave and Grossman, Ari and Guti{\'{e}}rrez, Mercedes and Fleagle, John G.},
doi = {10.1002/evan.20338},
file = {:C$\backslash$:/Users/Jule/Documents/Uni/MA/References{\_}MfN/MA{\_}not useful/Leakey{\_}et{\_}al-2011-Evolutionary{\_}Anthropology-{\_}Issues,{\_}News,{\_}and{\_}Reviews.pdf:pdf},
isbn = {1520-6505 (Electronic)$\backslash$n1060-1538 (Linking)},
issn = {10601538},
journal = {Evolutionary Anthropology},
keywords = {Ethiopia,Fossil,Kenya,Mammals,Primates},
number = {6},
pages = {238--253},
pmid = {22170693},
title = {{Faunal Change in the Turkana Basin during the Late Oligocene and Miocene}},
volume = {20},
