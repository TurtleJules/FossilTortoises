@article{Palkovacs2002,
abstract = {Today, the only surviving wild population of giant tortoises in the Indian Ocean occurs on the island of Aldabra. However, giant tortoises once inhabited islands throughout the western Indian Ocean. Madagascar, Africa, and India have all been suggested as possible sources of colonization for these islands. To address the origin of Indian Ocean tortoises (Dipsochelys, formerly Geochelone gigantea), we sequenced the 12S, 16S, and cyt b genes of the mitochondrial DNA. Our phylogenetic analysis shows Dipsochelys to be embedded within the Malagasy lineage, providing evidence that Indian Ocean giant tortoises are derived from a common Malagasy ancestor. This result points to Madagascar as the source of colonization for western Indian Ocean islands by giant tortoises. Tortoises are known to survive long oceanic voyages by floating with ocean currents, and thus, currents flowing northward towards the Aldabra archipelago from the east coast of Madagascar would have provided means for the colonization of western Indian Ocean islands. Additionally, we found an accelerated rate of sequence evolution in the two Malagasy Pyxis species examined. This finding supports previous theories that shorter generation time and smaller body size are related to an increase in mitochondrial DNA substitution rate in vertebrates. {\textcopyright} 2002 Elsevier Science (USA). All rights reserved.},
author = {Palkovacs, Eric P. and Gerlach, Justin and Caccone, Adalgisa},
doi = {10.1016/S1055-7903(02)00211-7},
file = {:C$\backslash$:/Users/Jule/Documents/Uni/MA/References{\_}MfN/Palkovacs{\_}2002{\_}Evolutionary origin of Indian Ocean Tortoises Dipsochelys.pdf:pdf},
isbn = {1055-7903},
issn = {10557903},
journal = {Molecular Phylogenetics and Evolution},
keywords = {Aldabra,Dipsochelys,Giant tortoises,Island colonization,Mitochondrial DNA,Pyxis,Rate variation},
number = {2},
pages = {216--227},
pmid = {12144758},
title = {{The evolutionary origin of Indian Ocean tortoises (Dipsochelys)}},
volume = {24},
year = {2002}
}
@article{Smith2016,
abstract = {The Geozoic encompasses the 3.6 Ga interval in Earth history when life has existed. Over this time, life has diversified from exclusively tiny, single-celled organisms to include large, complex multicellular forms. Just how and why this diversification occurred has been a major area of interest for paleontologists and evolutionary biologists for centuries. Here, we compile data on organism size throughout the Geozoic fossil record for the three domains of life. We describe canonical trends in the evolution of body size, synthesize current understanding of the patterns and causal mechanisms at various hierarchical scales, and discuss the biological and geological consequences of variation in organismal size.},
author = {Smith, Felisa A. and Payne, Jonathan L. and Heim, Noel A. and Balk, Meghan A. and Finnegan, Seth and Kowalewski, Micha{\l} and Lyons, S. Kathleen and McClain, Craig R. and McShea, Daniel W. and Novack-Gottshall, Philip M. and Anich, Paula Spaeth and Wang, Steve C.},
