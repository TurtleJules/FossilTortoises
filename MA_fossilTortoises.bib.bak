@article{DeLapparentdeBroin2000,
abstract = {The five major phases in the palaeontological history of African chelonians are presented: 1) autochthonous development of the north Gondwanan pleurodires from a Pangean source group; 2) littoral expansion of a member of this group (Bothremydidae), accompanied by the arrival of Laurasian marine turtles; 3) in situ development of pleurodires and the immigration of Eurasian cryptodires (Oligo-Miocene) traversing the Tethys in several waves; 4) great diversification and endemism. (Pliocene to Holocene); 5) important faunal reduction due to climatic changes at the end of Holocene times (cooling, aridification); elsewhere, great speciation and arrival during the Present of the last European immigrant in the north. Throughout the period under consideration there were several reductions in taxonomic diversity and emigrations from Africa. A preliminary catalogue of the fossil record of African chelonians is given, presented country by country followed by a taxonomic listing.},
author = {{De Lapparent de Broin}, F.},
file = {:C$\backslash$:/Users/Jule/Documents/Uni/MA/References{\_}MfN/Lapparent de Broin{\_}2000{\_}African Chelonians from the Jurassic to the Present.pdf:pdf},
isbn = {(print) 0078-8554},
issn = {00788554},
journal = {Palaeontologia Africana},
keywords = {Africa,Fossil chelonians,Stratigraphy,Taxonomy},
number = {36},
pages = {43--82},
title = {{African chelonians from the Jurassic to the present: Phases of development and preliminary catalogue of the fossil record}},
volume = {36},
year = {2000}
}
