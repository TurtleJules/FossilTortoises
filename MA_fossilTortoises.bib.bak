keywords = {catalonia,emys,fossils,mauremys,quaternary,testudo},
number = {May},
title = {{The fossil register of the genera Testudo, Emys and Mauremys of the Quaternary in Catalonia}},
url = {http://vipersgarden.at/PDF{\_}files/PDF-2485.pdf},
volume = {4},
year = {2006}
