keywords = {11 Vertebrate paleontology,Anapsida,Cenozoic,Charleston County South Carolina,Chordata,Colleton County South Carolina,Eastern U.S.,Florida,Georgia,Gopherus polyphemus,Horry County South Carolina,Lowndes County Mississippi,Mississippi,Neogene,Pleistocene,Pliocene,Quaternary,Reptilia,South Carolina,Southeastern U.S.,Tertiary,Testudines,Tetrapoda,United States,Vertebrata,archaeology,biogeography,human activity,paleoecology,shells,size,upper Pliocene},
pages = {179--199},
title = {{A fossil and zooarchaeological history of the gopher tortoise (Gopherus polyphemus) in the Southeastern United States}},
