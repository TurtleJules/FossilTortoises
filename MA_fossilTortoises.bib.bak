file = {:C$\backslash$:/Users/Jule/Documents/Uni/MA/References{\_}MfN/MA{\_}Intro-Discussion{\_}only/Smith{\_}2016{\_}Body Size Evolution Across the Geozoic.pdf:pdf},
isbn = {0084-65971545-4495},
issn = {0084-6597},
journal = {Annual Review of Earth and Planetary Sciences},
keywords = {allometry,biovolume,geobiology,macroevolution,maximum body size},
number = {1},
pages = {523--553},
title = {{Body Size Evolution Across the Geozoic}},
url = {http://www.annualreviews.org/doi/10.1146/annurev-earth-060115-012147},
volume = {44},
year = {2016}
}
