journal = {Mycologist},
keywords = {agarics,biodiversity,boleti,caledonian pine forest,curve,list of the fungi,must have a good,one,present,quality of a site,species accumulation,to assess the mycological},
number = {3},
pages = {98--102},
publisher = {British Mycological Society},
title = {{The species accumulation curve for agarics and boleti from a Caledonian pinewood}},
