pages = {1--16},
title = {{Pleistocene vertebrates from Celebes : XIV. Additions to the Archidiskodon-Celebochoerus fauna}},
url = {http://www.repository.naturalis.nl/record/319341{\%}5Cnhttp://www.repository.naturalis.nl/document/150627},
