@article{Olson2006,
abstract = {Paleontological and geological evidence suggest that the distinctive$\backslash$nendemic skink Eumeces longirostris could potentially be as old as$\backslash$ncontinuously emergent land on the Bermuda seamount (approximately$\backslash$n{\textgreater} 1-2 million yr). The species has experienced sustained evolutionary$\backslash$nstasis for at least the past 400,000 yr, during which time there$\backslash$nhas been no perceptible change in skeletal morphology. The tortoise$\backslash$nHesperotestudo bermudae is known from a single fossil from interglacial$\backslash$nperiod marine isotope stage (MIS) 9-approximately 300,000 yr ago.$\backslash$nA viable population of tortoise on Bermuda could be reconciled with$\backslash$nthe geological record and the lack of any other fossils of the species$\backslash$nto date if tortoises colonized the island at or after the end of$\backslash$nthe maximal sea-level rise of interglacial MIS 11, evolved during$\backslash$nthe single glacial episode of MIS 10, and become extinct as a result$\backslash$nof the interglacial sea-level rise of MIS 9, a period of about 100,000$\backslash$nyr. Such rapid evolution and extinction has a close parallel in$\backslash$nthe giant tortoises of Aldabra Island in the Indian Ocean},
author = {Olson, Storrs L. and Hearty, Paul J. and Pregill, Gregory K.},
doi = {10.1670/0022-1511(2006)40[394:GCOEAS]2.0.CO;2},
file = {:C$\backslash$:/Users/Jule/Documents/Uni/MA/References{\_}MfN/MA{\_}Intro-Discussion{\_}only/Olson{\_}2006{\_}Geological Constraints on Evolution and Survival in Endemic Reptiles on Bermuda.pdf:pdf},
isbn = {9212102492},
issn = {0022-1511},
journal = {Journal of Herpetology},
number = {3},
pages = {394--398},
title = {{Geological Constraints on Evolution and Survival in Endemic Reptiles on Bermuda}},
volume = {40},
year = {2006}
}
@article{Joyce2007,
abstract = {Abstract Presented herein is the first morphological analysis of turtle relationships to examine the monophyly of many turtle groups by using only single species as terminals and by integrating a large number of primitive fossil taxa. The data matrix consists of 136 osteological parsimony informative characters with 169 derived character states for 45 fossil and 22 living species of the clade Testudinata. The results corroborate the monophyly of a large number of previously hypothesized clades, but refute the accepted hypothesis regarding the basal split of living turtles. In particular, the primitive turtles Proterochersis robusta, Kayentachelys aprix, Mongolochelys efremovi, Meiolania platyceps, and Kallokibotion bajazidi are removed from their current position as crown turtles and placed along the phylogenetic stem of this clade. The age of the turtle crown is thereby adjusted from the Late Triassic to the Late Jurassic, which is relevant to testing molecular clock hypotheses. This revised topology has...},
author = {Joyce, Walter G.},
doi = {10.3374/0079-032X(2007)48[3:PROMT]2.0.CO;2},
file = {:C$\backslash$:/Users/Jule/Documents/Uni/MA/References{\_}MfN/MA{\_}not useful/Joyce{\_}2007{\_}Phylogenetic relationshipf of mesozoic turtles.pdf:pdf},
isbn = {0079-032X},
issn = {0079-032X},
journal = {Bulletin of the Peabody Museum of Natural History},
keywords = {Cryptodira,Pancryptodira,Panpleurodira,Phylogeny,Pleurodira,Testudinata,Testudines,basal turtles,ecology,phylogenetic nomenclature,trochlear mechanism},
number = {1},
pages = {3--102},
title = {{Phylogenetic Relationships of Mesozoic Turtles}},
url = {http://www.bioone.org/doi/abs/10.3374/0079-032X{\%}282007{\%}2948{\%}5B3{\%}3APROMT{\%}5D2.0.CO{\%}3B2},
volume = {48},
year = {2007}
}
@article{Lomoschitz2016,
abstract = {The Famara massif, in the north of Lanzarote Island, constitutes the remains of a former island inhabited by the oldest known vertebrate fauna of the Canary archipelago off the coast of Africa. In this study, new ages are offered for the underlying and overlying basaltic lava flows of two paleontological sites. The island's three major palaeontological sites, which contain remains of this ancient fauna (Valle Grande, Valle Chico and Fuente de Gusa), are intercorrelated according to their lithologies, sedimentology, palaeontological content and geochronology. The new K/Ar age interval for the fossiliferous sedimentary deposits ranges between 4.3??????0.7 and 3.78??????0.71 Ma, within the Early Pliocene, and shows that the first known terrestrial animals in Lanzarote were present on the island for about 500 ka. The principal component of the deposits is a bioclastic calcarenite of aeolian origin (sand sheet deposits), which is present in all three sites and constitutes 65{\%} of the beds. The remaining 35{\%} is of fluvial-aeolian origin (mainly stream deposits). All the beds contain the same fossils (insect egg pods, land snails, avian eggshells and tortoise eggshells). The local palaeogeography and the formation of the deposits were conditioned by a flat plain, larger than 16??km2, over which aeolian sands moved freely with a prevailing NNE-WSW wind direction. In agreement with previous investigations, the palaeoclimate in this interval (ca. 4.3 to 3.8 Ma) must have been mainly dry with some rainy episodes.},
author = {Lomoschitz, Alejandro and {S??nchez Marco}, Antonio and Huertas, Mar??a Jos?? and Betancort, Juan F. and Isern, Arnau and Sanz, Elena and Meco, Joaqu??n},
doi = {10.1016/j.jafrearsci.2016.08.006},
file = {:C$\backslash$:/Users/Jule/Documents/Uni/MA/References{\_}MfN/Lomoschitz{\_}2016{\_}Lanzarote Island.pdf:pdf},
issn = {18791956},
journal = {Journal of African Earth Sciences},
keywords = {Canary Islands,Fossil egg,K-Ar ages,Oceanic Island,Pliocene,Terrestrial fossil fauna},
pages = {338--349},
title = {{A reappraisal of the stratigraphy and chronology of Early Pliocene palaeontological sites from Lanzarote Island containing fossil terrestrial animals}},
volume = {123},
year = {2016}
}
