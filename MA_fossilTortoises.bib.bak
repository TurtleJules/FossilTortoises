@misc{Palmer1992,
abstract = {Fluctuating asymmetry (FA) is a widely used measure of developmental stability. Nearly all FA indexes estimate the variance of the frequency distribution of right-minus-left (R-L) for a given bilateral character. Differences in these indexes among samples are usually interpreted as reflecting differences in the level of developmental stability. If developmental stabilty is the ability to correct for small, random developmental pertrubations of exclusively environmental origin, then a distribution of R-L, which may include both genetically and environmentally caused asymmetries, may not be a good measure of developmental stability. R-L distributions that depart significantly from a statistical criteria for ideal FA (mean of zero, normal distribution) are unsuitable as descriptors of developmental stability because a fraction of the asymmetry variation may have a genetic basis. In addition, broad-peaked or bimodal (platykurtic) distirubitons of R-L, which reveal the presence of antisymmetry, also imply genetically based asymmetries and thus seem inappropriate as descriptors of developmental stability. Finally, both skewed distributions and narrow-peaked, long-tailed (leptokurtic) distirubtions may arise in mixed samples composed of two or more groups of individuals where each exhibits a different form of asymmetry and where one or more forms of asymmetry may have a genetic basis. Statistical techniques for detecting departures from normality and for detecting heterogeneity of variances among samples are briefly reviewed.},
author = {Palmer, A. Richard and Strobeck, C.},
booktitle = {Acta Zoologica Fennica},
doi = {10.1348/000712601162284},
file = {:C$\backslash$:/Users/Jule/Documents/Uni/MA/References{\_}MfN/MA{\_}Analysis/Palmer{\_}Strobeck{\_}1989{\_}Fluctuating asymmetry as a measure of developmental stability - implications of non-normal distributions and power of statistical tests.pdf:pdf},
isbn = {9519481362},
issn = {0001-7299},
keywords = {antisymmetry,directional asymmetry,fluctuating asymmetry},
pages = {57--72},
title = {{Fluctuating asymmetry as a measure of developmental stability: implications of non-normal distributions and power of statistical tests}},
volume = {191},
year = {1992}
}
@article{Gotelli2001,
abstract = {Species richness is a fundamental measurement of community and regional diversity, and it underlies many ecological models and conservation strategies. In spite of its importance, ecologists have not always appreciated the effects of abundance and sampling effort on richness measures and comparisons. We survey a series of common pitfalls in quantifying and comparing taxon richness. These pitfalls can be largely avoided by using accumulation and rarefaction curves, which may be based on either individuals or samples. These taxon sampling curves contain the basic information for valid richness comparisons, including category-subcategory ratios (species-to-genus and species-to-individual ratios). Rarefaction methods - both sample-based and individual-based - allow for meaningful standardization and comparison of datasets. Standardizing data sets by area or sampling effort may produce very different results compared to standardizing by number of individuals collected, and it is not always clear which measure of diversity is more appropriate. Asymptotic richness estimators provide lower-bound estimates for taxon-rich groups such as tropical arthropods, in which observed richness rarely reaches an asymptote, despite intensive sampling. Recent examples of diversity studies of tropical trees, stream invertebrates, and herbaceous plants emphasize the importance of carefully quantifying species richness using taxon sampling curves.},
author = {Gotelli, N J and Colwell, R K},
doi = {10.1046/j.1461-0248.2001.00230.x},
file = {:C$\backslash$:/Users/Jule/Documents/Uni/MA/References{\_}MfN/MA{\_}SpeciesAccumulationCurve{\_}genera/Gotelli et al{\_}2001{\_}Quantifying biodiversity procedures and pitfalls in the measurement and comparison of species richness.pdf:pdf},
isbn = {1461-023X},
issn = {1461023X},
journal = {Ecology Letters},
keywords = {biodiversity,diversity,model,plant,sampling},
number = {4},
pages = {379--391},
pmid = {223},
title = {{Quantifyinf Biodiversity: Procedures and Pitfalls in the Measurement and Comparison of Species Richness}},
volume = {4},
year = {2001}
}
