@article{Hofmeyr2004,
abstract = {The reproductive cycle and egg production of Chersina angulata females,$\backslash$nmaintained under natural climate conditions, were studied over 29$\backslash$nmonths using ultrasonography. Angulate tortoises initiated egg production$\backslash$nin February (late summer) and were gravid through most of the year.$\backslash$nA short nongravid period in January indicates a cyclic rather than$\backslash$ncontinuous reproductive pattern. Chersina angulata females produced$\backslash$none egg at a time, and clutch frequency ranged from one to six per$\backslash$nyear. Annual fecundity was influenced by the date of reproductive$\backslash$nonset and egg-retention time; early ovulation and a short retention$\backslash$ntime increased annual fecundity. Egg retention time was highly variable$\backslash$n(23-212 days) and correlated to ambient temperature (inversely)$\backslash$nand rainfall (positively). Temperature had no direct effect on oviposition,$\backslash$nbut rainfall served as an important exogenous cue, perhaps facilitating$\backslash$nnesting. Oviposition and ovulation were synchronized and females$\backslash$nprepared a new egg immediately after oviposition. This reproductive$\backslash$npattern is highly unusual for chelonians in Mediterranean climates.$\backslash$nThis pattern might facilitate reproduction in the different environments$\backslash$nover the range of C. angulata},
author = {Hofmeyr, M D},
doi = {10.2307/1566211},
file = {:C$\backslash$:/Users/Jule/Documents/Uni/MA/References{\_}MfN/MA{\_}Intro-Discussion{\_}only/Hofmeyr{\_}2004{\_}Egg production in Chersina angulata unusual pattern in mediterranean climate.pdf:pdf},
isbn = {0022-1511},
issn = {00221511},
journal = {Journal of Herpetology},
keywords = {CLUTCH,CUE,CYCLE,EGG,EGG-PRODUCTION,ENVIRONME},
number = {2},
pages = {172--179},
title = {{Egg production in {\textless}i{\textgreater}Chersina angulata{\textless}/i{\textgreater}: An unusual pattern in a Mediterranean climate}},
url = {http://www.bioone.org/doi/abs/10.1670/133-03A{\%}0Apapers3://publication/doi/10.1670/133-03A},
